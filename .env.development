# CulturaFlow Development Environment

# API Configuration
API_BASE_URL=http://192.168.1.76:3001
WEBSOCKET_URL=ws://192.168.1.76:3001

# App Configuration
APP_ENV=development
APP_VERSION=1.0.0
DEBUG_MODE=true
LOG_LEVEL=debug

# Feature Flags - ACTIVADAS PARA TESTING COMPLETO
ENABLE_OFFLINE_MODE=true
ENABLE_GAMIFICATION=true
ENABLE_SOCIAL_FEATURES=true
ENABLE_AI_CONTENT=true
ENABLE_ANALYTICS=true

# Testing & Demo
DEMO_MODE=true
MOCK_API_DELAY=1000
ENABLE_TEST_ACCOUNTS=true

# Expo Configuration
EXPO_PROJECT_ID=your-expo-project-id-here